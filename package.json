        "eslint-plugin-markdown": "1.0.0-beta.8",
        "eslint-plugin-react": "7.5.1",
        "eslint-tinker": "0.4.0",
        "etool-build": "1.0.6",
        "express": "4.16.3",
        "extract-text-webpack-plugin": "4.0.0-beta.0",
        "file-loader": "4.2.0",
        "happypack": "5.0.0",
        "html-loader": "0.5.5",
        "html-webpack-plugin": "3.2.0",
        "http-proxy-middleware": "0.19.0",
        "less": "3.9.0",
