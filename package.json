        "draftjs-to-html": "0.8.4",
        "echarts-for-react": "2.0.8",
        "es3ify-webpack-plugin-v2": "0.0.2",
        "es5-shim": "4.5.10",
        "es6-promise": "4.2.4",
        "event-source-polyfill": "0.0.12",
