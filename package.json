{
    "name": "website",
    "version": "1.0.0",
    "description": "enterprise develop framework",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=production webpack-dev-server --inline --progress --colors --compress --config ./build/webpack.dev.js",
        "start": "cross-env NODE_ENV=production webpack-dev-server --inline --progress --colors --watch --compress --content-base ./dist --port 8081 --host 0.0.0.0",
        "release": "npm run build && npm run splitcss",
        "packmodule": "cross-env NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.config.prd.js && cross-env NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.splitcss.js",
        "build": "cross-env NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.config.prd.js",
        "build-nohash": "cross-env NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.config.nohash.prd.js",
        "css": "cross-env NODE_ENV=production webpack --progress --config webpack.config.css.prd.js",
        "splitcss": "cross-env NODE_ENV=production webpack --progress --config webpack.splitcss.js",
        "splitcss-nohash": "cross-env NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.nohash.splitcss.js",
        "dll": "cross-env NODE_ENV=production webpack --config webpack.dll.js",
        "dll:edf": "cross-env NODE_ENV=production webpack --config ./build/webpack.edf.dll.js",
        "dll:dev": "cross-env NODE_ENV=production webpack --mode production  --progress --colors --config webpack.dev.dll.js",
        "theme": "cross-env NODE_ENV=production webpack --watch --config ./build/webpack.theme.js",
        "module": "cross-env NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.config.module.js",
        "module:cli": "npm run build && npm run css && npm run css --split",
        "modules": "npm run module --access && npm run module --asset && npm run module --ba && npm run module --edf && npm run module --gl && npm run module --operation && npm run module --operationbig && npm run module --scm && npm run module --taxapply && npm run module --taxcheckup && npm run module --zzssb && npm run module --xc && npm run module --bill && npm run module --invoices && npm run module --taxclient && npm run module --yygl && npm run module --stock && npm run module --bovms && npm run module --contract",
        "modules:all": "npm run build && npm run css && npm run css --split && npm run modules && node createManifest.js",
        "merge:manifest": "node createManifest.js",
        "shim": "cross-env NODE_ENV=production webpack --config webpack.shim.js",
        "single": "cross-env NODE_ENV=production webpack-dev-server --inline --progress --colors --watch --compress --content-base ./dist --port 8081 --host 0.0.0.0 --config webpackSingleApp/webpack.config.single.js",
        "singleBuild": "cross-env NODE_ENV=single webpack --progress --colors --display-error-details --config webpack.config.prd.js",
        "singleCss": "cross-env NODE_ENV=single webpack --progress --config webpack.config.css.prd.js",
        "singleStatic": "npm run singleBuild && npm run singleCss && npm run modules && npm run merge:manifest",
        "stats": "webpack NODE_ENV=production -config webpack.config.prd.js --profile --json > stats.json",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint apps",
        "less": "lessc  --clean-css ./assets/styles/ie.less ./vendor/ie.css",
        "help": "echo \" \r\n***********按模块打包使用方式:如 npm run start --arg=\"edf scm\" ",
        "modules:test": "npm run build && npm run css && npm run module --xc && node createManifest.js"
    },
    "keywords": [
        "react",
        "reactjs",
        "flux",
        "redux",
        "react-redux",
        "antd",
        "immutable"
    ],
    "author": "edf team",
    "license": "MIT",
    "babel": {
        "presets": [
            "es2015",
            "react",
            "stage-0"
        ],
        "plugins": [
            "babel-plugin-transform-decorators-legacy",
            [
                "import",
                {
                    "libraryName": "antd",
                    "style": true
                }
            ]
        ]
    },
    "dependencies": {
        "antd": "3.8.4",
        "babel-polyfill": "6.26.0",
