        "react-dnd": "3.0.2",
        "react-dnd-html5-backend": "7.4.4",
        "react-dom": "16.13.1",
        "react-draft-wysiwyg": "1.12.13",
        "react-json-tree": "0.11.0",
        "react-keydown": "1.9.11",
        "react-redux": "5.0.7",
        "react-resizable": "1.7.5",
        "react-viewer": "2.3.9",
        "redux": "3.7.2",
        "ttk-app-init-demo": "^2.1.2",
        "ttk-rc-intro": "1.0.4",
